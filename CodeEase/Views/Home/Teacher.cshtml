@{
    ViewData["Title"] = "Teacher Dashboard";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="d-flex justify-content-between align-items-center mb-4">
                <h2><i class="fas fa-chalkboard-teacher me-2"></i>Teacher Dashboard</h2>
                <div class="btn-group" role="group">
                    <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#createClassModal">
                        <i class="fas fa-plus me-1"></i>New Class
                    </button>
                    <button type="button" class="btn btn-success" data-bs-toggle="modal" data-bs-target="#createLessonModal">
                        <i class="fas fa-plus me-1"></i>New Lesson
                    </button>
                    <button type="button" class="btn btn-info" data-bs-toggle="modal" data-bs-target="#quizCreationModal">
                        <i class="fas fa-question-circle me-1"></i>New Quiz
                    </button>
                </div>
            </div>

            <!-- Dashboard Stats -->
            <div class="row mb-4" id="dashboardStats">
                <div class="col-md-3">
                    <div class="card bg-primary text-white">
                        <div class="card-body">
                            <div class="d-flex justify-content-between">
                                <div>
                                    <h4 id="totalClasses">0</h4>
                                    <p class="mb-0">Total Classes</p>
                                </div>
                                <i class="fas fa-users fa-2x opacity-75"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card bg-success text-white">
                        <div class="card-body">
                            <div class="d-flex justify-content-between">
                                <div>
                                    <h4 id="totalStudents">0</h4>
                                    <p class="mb-0">Total Students</p>
                                </div>
                                <i class="fas fa-user-graduate fa-2x opacity-75"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card bg-info text-white">
                        <div class="card-body">
                            <div class="d-flex justify-content-between">
                                <div>
                                    <h4 id="totalLessons">0</h4>
                                    <p class="mb-0">Total Lessons</p>
                                </div>
                                <i class="fas fa-book fa-2x opacity-75"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card bg-warning text-white">
                        <div class="card-body">
                            <div class="d-flex justify-content-between">
                                <div>
                                    <h4 id="publishedLessons">0</h4>
                                    <p class="mb-0">Published Lessons</p>
                                </div>
                                <i class="fas fa-check-circle fa-2x opacity-75"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Navigation Tabs -->
            <ul class="nav nav-tabs mb-4" id="teacherTabs" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link active" id="classes-tab" data-bs-toggle="tab" data-bs-target="#classes" type="button" role="tab">
                        <i class="fas fa-users me-1"></i>Classes
                    </button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="lessons-tab" data-bs-toggle="tab" data-bs-target="#lessons" type="button" role="tab">
                        <i class="fas fa-book me-1"></i>Lessons
                    </button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="quizzes-tab" data-bs-toggle="tab" data-bs-target="#quizzes" type="button" role="tab">
                        <i class="fas fa-question-circle me-1"></i>Quizzes
                    </button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="analytics-tab" data-bs-toggle="tab" data-bs-target="#analytics" type="button" role="tab">
                        <i class="fas fa-chart-bar me-1"></i>Analytics
                    </button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="progress-tab" data-bs-toggle="tab" data-bs-target="#progress" type="button" role="tab">
                        <i class="fas fa-chart-line me-1"></i>Progress
                    </button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="ai-tools-tab" data-bs-toggle="tab" data-bs-target="#ai-tools" type="button" role="tab">
                        <i class="fas fa-robot me-1"></i>AI Tools
                    </button>
                </li>
            </ul>

            <!-- Tab Content -->
            <div class="tab-content" id="teacherTabContent">
                <!-- Classes Tab -->
                <div class="tab-pane fade show active" id="classes" role="tabpanel">
                    <div class="row" id="classesContainer">
                        <!-- Classes will be loaded here -->
                    </div>
                </div>

                <!-- Lessons Tab -->
                <div class="tab-pane fade" id="lessons" role="tabpanel">
                    <div class="d-flex justify-content-between align-items-center mb-3">
                        <h4>My Lessons</h4>
                        <div class="input-group" style="width: 300px;">
                            <input type="text" class="form-control" id="lessonSearch" placeholder="Search lessons...">
                            <button class="btn btn-outline-secondary" type="button">
                                <i class="fas fa-search"></i>
                            </button>
                        </div>
                    </div>
                    <div class="row" id="lessonsContainer">
                        <!-- Lessons will be loaded here -->
                    </div>
                </div>

                <!-- Quizzes Tab -->
                <div class="tab-pane fade" id="quizzes" role="tabpanel">
                    <div class="d-flex justify-content-between align-items-center mb-3">
                        <h4>My Quizzes</h4>
                        <div class="d-flex gap-2">
                            <div class="input-group" style="width: 300px;">
                                <input type="text" class="form-control" id="quizSearch" placeholder="Search quizzes...">
                                <button class="btn btn-outline-secondary" type="button">
                                    <i class="fas fa-search"></i>
                                </button>
                            </div>
                            <div class="dropdown">
                                <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown">
                                    <i class="fas fa-filter me-1"></i>Filter
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item" href="#" data-filter="all">All Quizzes</a></li>
                                    <li><a class="dropdown-item" href="#" data-filter="published">Published</a></li>
                                    <li><a class="dropdown-item" href="#" data-filter="draft">Draft</a></li>
                                    <li><a class="dropdown-item" href="#" data-filter="archived">Archived</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="row" id="quizzesContainer">
                        <!-- Quizzes will be loaded here -->
                    </div>
                </div>

                <!-- Analytics Tab -->
                <div class="tab-pane fade" id="analytics" role="tabpanel">
                    <div class="row mb-4">
                        <div class="col-12">
                            <h4><i class="fas fa-chart-bar me-2"></i>Quiz Analytics & Reports</h4>
                            <p class="text-muted">View detailed analytics and performance reports for your quizzes.</p>
                        </div>
                    </div>
                    
                    <div class="row mb-4">
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header">
                                    <h6 class="mb-0"><i class="fas fa-filter me-2"></i>Filter Options</h6>
                                </div>
                                <div class="card-body">
                                    <div class="mb-3">
                                        <label for="analyticsClassSelect" class="form-label">Select Class</label>
                                        <select class="form-select" id="analyticsClassSelect">
                                            <option value="">All Classes</option>
                                        </select>
                                    </div>
                                    <div class="mb-3">
                                        <label for="analyticsDateRange" class="form-label">Date Range</label>
                                        <select class="form-select" id="analyticsDateRange">
                                            <option value="7">Last 7 days</option>
                                            <option value="30" selected>Last 30 days</option>
                                            <option value="90">Last 90 days</option>
                                            <option value="365">Last year</option>
                                        </select>
                                    </div>
                                    <button type="button" class="btn btn-primary" onclick="loadQuizSummaries()">
                                        <i class="fas fa-sync me-1"></i>Refresh Data
                                    </button>
                                </div>
                            </div>
                        </div>
                        
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header">
                                    <h6 class="mb-0"><i class="fas fa-chart-pie me-2"></i>Quick Stats</h6>
                                </div>
                                <div class="card-body">
                                    <div class="row text-center">
                                        <div class="col-6">
                                            <h4 class="text-primary mb-0" id="totalQuizzes">0</h4>
                                            <small class="text-muted">Total Quizzes</small>
                                        </div>
                                        <div class="col-6">
                                            <h4 class="text-success mb-0" id="totalAttempts">0</h4>
                                            <small class="text-muted">Total Attempts</small>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row text-center">
                                        <div class="col-6">
                                            <h4 class="text-info mb-0" id="averageScore">0%</h4>
                                            <small class="text-muted">Average Score</small>
                                        </div>
                                        <div class="col-6">
                                            <h4 class="text-warning mb-0" id="completionRate">0%</h4>
                                            <small class="text-muted">Completion Rate</small>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="card">
                        <div class="card-header d-flex justify-content-between align-items-center">
                            <h6 class="mb-0"><i class="fas fa-list me-2"></i>Quiz Performance Summary</h6>
                            <button type="button" class="btn btn-sm btn-outline-primary" onclick="exportAnalyticsReport()">
                                <i class="fas fa-download me-1"></i>Export Report
                            </button>
                        </div>
                        <div class="card-body">
                            <div id="quizSummariesContainer">
                                <div class="text-center text-muted py-4">
                                    <i class="fas fa-chart-bar fa-3x mb-3"></i>
                                    <p>Click "Refresh Data" to load quiz analytics.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Progress Tab -->
                <div class="tab-pane fade" id="progress" role="tabpanel">
                    <div class="row">
                        <div class="col-md-6">
                            <h4>Select Class</h4>
                            <select class="form-select mb-3" id="progressClassSelect">
                                <option value="">Select a class to view progress...</option>
                            </select>
                        </div>
                    </div>
                    <div id="progressContainer">
                        <!-- Progress data will be loaded here -->
                    </div>
                </div>

                <!-- AI Tools Tab -->
                <div class="tab-pane fade" id="ai-tools" role="tabpanel">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header">
                                    <h5><i class="fas fa-magic me-2"></i>Generate Lesson Content</h5>
                                </div>
                                <div class="card-body">
                                    <form id="generateLessonForm">
                                        <div class="mb-3">
                                            <label for="lessonTopic" class="form-label">Topic</label>
                                            <input type="text" class="form-control" id="lessonTopic" placeholder="e.g., Java Arrays, Python Functions">
                                        </div>
                                        <div class="mb-3">
                                            <label for="lessonDifficulty" class="form-label">Difficulty</label>
                                            <select class="form-select" id="lessonDifficulty">
                                                <option value="0">Beginner</option>
                                                <option value="1">Intermediate</option>
                                                <option value="2">Advanced</option>
                                            </select>
                                        </div>
                                        <button type="submit" class="btn btn-primary">
                                            <i class="fas fa-wand-magic-sparkles me-1"></i>Generate Content
                                        </button>
                                    </form>
                                    <div id="generatedContent" class="mt-3" style="display: none;">
                                        <h6>Generated Content:</h6>
                                        <div class="border p-3 bg-light" id="contentPreview"></div>
                                        <button type="button" class="btn btn-success mt-2" id="useGeneratedContent">
                                            Use This Content
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header">
                                    <h5><i class="fas fa-question-circle me-2"></i>Generate Quiz Questions</h5>
                                </div>
                                <div class="card-body">
                                    <form id="generateQuizForm">
                                        <div class="mb-3">
                                            <label for="quizLessonSelect" class="form-label">Select Lesson</label>
                                            <select class="form-select" id="quizLessonSelect">
                                                <option value="">Choose a lesson...</option>
                                            </select>
                                        </div>
                                        <div class="mb-3">
                                            <label for="questionCount" class="form-label">Number of Questions</label>
                                            <input type="number" class="form-control" id="questionCount" value="5" min="1" max="20">
                                        </div>
                                        <button type="submit" class="btn btn-primary">
                                            <i class="fas fa-wand-magic-sparkles me-1"></i>Generate Questions
                                        </button>
                                    </form>
                                    <div id="generatedQuestions" class="mt-3" style="display: none;">
                                        <h6>Generated Questions:</h6>
                                        <div id="questionsPreview"></div>
                                        <button type="button" class="btn btn-success mt-2" id="useGeneratedQuestions">
                                            Add to Quiz
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Create Class Modal -->
<div class="modal fade" id="createClassModal" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Create New Class</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <form id="createClassForm">
                <div class="modal-body">
                    <div class="mb-3">
                        <label for="className" class="form-label">Class Name</label>
                        <input type="text" class="form-control" id="className" required>
                    </div>
                    <div class="mb-3">
                        <label for="classDescription" class="form-label">Description</label>
                        <textarea class="form-control" id="classDescription" rows="3"></textarea>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Create Class</button>
                </div>
            </form>
        </div>
    </div>
</div>

<!-- Create Lesson Modal -->
<div class="modal fade" id="createLessonModal" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Create New Lesson</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <form id="createLessonForm">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="mb-3">
                                <label for="lessonTitle" class="form-label">Lesson Title</label>
                                <input type="text" class="form-control" id="lessonTitle" required>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="mb-3">
                                <label for="lessonDifficultySelect" class="form-label">Difficulty</label>
                                <select class="form-select" id="lessonDifficultySelect" required>
                                    <option value="0">Beginner</option>
                                    <option value="1">Intermediate</option>
                                    <option value="2">Advanced</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="mb-3">
                        <label for="lessonDescriptionInput" class="form-label">Description</label>
                        <textarea class="form-control" id="lessonDescriptionInput" rows="2"></textarea>
                    </div>
                    <div class="mb-3">
                        <label for="lessonContent" class="form-label">Content</label>
                        <textarea class="form-control" id="lessonContent" rows="8" required></textarea>
                    </div>
                    <div class="mb-3">
                        <label for="codeTemplate" class="form-label">Code Template (Optional)</label>
                        <textarea class="form-control" id="codeTemplate" rows="4" placeholder="// Starter code for students"></textarea>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Create Lesson</button>
                </div>
            </form>
        </div>
    </div>
</div>

<!-- Class Detail Modal -->
<div class="modal fade" id="classDetailModal" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="classDetailTitle">Class Details</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div id="classDetailContent">
                    <!-- Class details will be loaded here -->
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Lesson Detail Modal -->
<div class="modal fade" id="lessonDetailModal" tabindex="-1">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="lessonDetailTitle">Lesson Details</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div id="lessonDetailContent">
                    <!-- Lesson details will be loaded here -->
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Quiz Creation Modal -->
<div class="modal fade" id="quizCreationModal" tabindex="-1">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="quizCreationTitle">Create New Quiz</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <form id="quizCreationForm">
                    <!-- Quiz Basic Information -->
                    <div class="row mb-4">
                        <div class="col-md-8">
                            <div class="mb-3">
                                <label for="quizTitleInput" class="form-label">Quiz Title</label>
                                <input type="text" class="form-control" id="quizTitleInput" required>
                            </div>
                            <div class="mb-3">
                                <label for="quizDescriptionInput" class="form-label">Description</label>
                                <textarea class="form-control" id="quizDescriptionInput" rows="2"></textarea>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="mb-3">
                                <label for="quizLessonSelectInput" class="form-label">Associated Lesson</label>
                                <select class="form-select" id="quizLessonSelectInput" required>
                                    <option value="">Select a lesson...</option>
                                </select>
                            </div>
                            <div class="mb-3">
                                <label for="quizTimeLimitInput" class="form-label">Time Limit (minutes)</label>
                                <input type="number" class="form-control" id="quizTimeLimitInput" value="30" min="5" max="180">
                            </div>
                        </div>
                    </div>

                    <!-- Quiz Questions Section -->
                    <div class="card">
                        <div class="card-header d-flex justify-content-between align-items-center">
                            <h6 class="mb-0"><i class="fas fa-question-circle me-2"></i>Quiz Questions</h6>
                            <div class="btn-group" role="group">
                                <button type="button" class="btn btn-sm btn-outline-primary" onclick="quizCreator.addQuestion('MultipleChoice')">
                                    <i class="fas fa-list-ul me-1"></i>Multiple Choice
                                </button>
                                <button type="button" class="btn btn-sm btn-outline-success" onclick="quizCreator.addQuestion('TrueFalse')">
                                    <i class="fas fa-check-circle me-1"></i>True/False
                                </button>
                                <button type="button" class="btn btn-sm btn-outline-info" onclick="quizCreator.addQuestion('CodeCompletion')">
                                    <i class="fas fa-code me-1"></i>Code Completion
                                </button>
                                <button type="button" class="btn btn-sm btn-outline-warning" onclick="quizCreator.addQuestion('ShortAnswer')">
                                    <i class="fas fa-edit me-1"></i>Short Answer
                                </button>
                            </div>
                        </div>
                        <div class="card-body">
                            <div id="quizQuestionsContainer">
                                <div class="text-center text-muted py-4">
                                    <i class="fas fa-question-circle fa-3x mb-3"></i>
                                    <p>No questions added yet. Click the buttons above to add questions.</p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- AI Generation Section -->
                    <div class="card mt-3">
                        <div class="card-header">
                            <h6 class="mb-0"><i class="fas fa-magic me-2"></i>AI Question Generation</h6>
                        </div>
                        <div class="card-body">
                            <div class="row">
                        <div class="col-md-8">
                            <div class="mb-3">
                                <label for="aiQuestionCount" class="form-label">Number of Questions</label>
                                <select class="form-select" id="aiQuestionCount">
                                    <option value="3">3 Questions</option>
                                    <option value="5" selected>5 Questions</option>
                                    <option value="10">10 Questions</option>
                                    <option value="15">15 Questions</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-4 d-flex align-items-end">
                            <button type="button" class="btn btn-primary w-100" onclick="quizCreator.generateAIQuestions()">
                                <i class="fas fa-magic me-2"></i>Generate Questions
                            </button>
                        </div>
                    </div>
                        </div>
                    </div>

                    <!-- Quiz Assignment Section -->
                    <div class="card mt-3">
                        <div class="card-header">
                            <h6 class="mb-0"><i class="fas fa-users me-2"></i>Quiz Assignment</h6>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="mb-3">
                                        <label for="assignToClasses" class="form-label">Assign to Classes</label>
                                        <select class="form-select" id="assignToClasses" multiple>
                                            <!-- Classes will be loaded here -->
                                        </select>
                                        <div class="form-text">Hold Ctrl/Cmd to select multiple classes</div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="mb-3">
                                        <label for="quizDueDate" class="form-label">Due Date (Optional)</label>
                                        <input type="datetime-local" class="form-control" id="quizDueDate">
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="publishImmediately" checked>
                                        <label class="form-check-label" for="publishImmediately">
                                            Publish immediately
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-outline-primary" onclick="quizCreator.previewQuiz()">
                    <i class="fas fa-eye me-1"></i>Preview Quiz
                </button>
                <button type="submit" class="btn btn-primary" form="quizCreationForm">
                    <i class="fas fa-save me-1"></i>Save Quiz
                </button>
            </div>
        </div>
    </div>
</div>

<!-- Quiz Preview Modal -->
<div class="modal fade" id="quizPreviewModal" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Quiz Preview</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div id="quizPreviewContent">
                    <!-- Quiz preview will be loaded here -->
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
